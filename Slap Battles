local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "üî¥BloodyHubüü• | Slap Battlesüëè", HidePremium = false, SaveConfig = true, IntroText = "Bloody Hub" ,ConfigFolder = "üî¥BloodyHubüü•"})

--Values
_G.Golden = true
_G.Nogolden = true
_G.Therock = true
_G.GhostON = true
_G.GhostOFF = true
_G.ByPass = true
_G.PhaseA = true
_G.AntiTime = true
_G.AntiMail = true
_G.Bob = true
_G.Royale = true
_G.HitBox = true
_G.Reach = true
_G.AutoUno = true
_G.FarmSlapples = true
_G.Slapples = true


--Functions
function Golden()
    local args = {
        [1] = true
    }
    
    game:GetService("ReplicatedStorage").Goldify:FireServer(unpack(args))
end

function Therock()
game:GetService("ReplicatedStorage").Rockmode:FireServer()
end

function NoGolden()
	local args = {
	    [1] = false,
	    [2] = BrickColor.new(1003)
	}
	
	game:GetService("ReplicatedStorage").Goldify:FireServer(unpack(args))
end

function GhostON()
	game:GetService("ReplicatedStorage").Ghostinvisibilityactivated:FireServer()
end

function GhostOFF()
	game:GetService("ReplicatedStorage").Ghostinvisibilitydeactivated:FireServer()
end

function Bypass()
		local Namecall
	Namecall = hookmetamethod(game, '__namecall', function(self, ...)
  	 if getnamecallmethod() == 'FireServer' and tostring(self) == 'Ban' then
  	     return
  	 elseif getnamecallmethod() == 'FireServer' and tostring(self) == 'WalkSpeedChanged' then
  	     return
 	  elseif getnamecallmethod() == 'FireServer' and tostring(self) == 'AdminGUI' then
 	      return
 	  end
 	  return Namecall(self, ...)
	end)
end

function PhaseA()
	game:GetService("ReplicatedStorage").PhaseA:FireServer()
end

function AntiTime()
	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end

function AntiMail()
	game.ReplicatedStorage.MailPopup:Destroy()
end

function Bob()
    local args = {
        [1] = true
    }
    
    game:GetService("ReplicatedStorage").Duplicate:FireServer(unpack(args))
    wait()
    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character,
        [2] = false
    }
    
    game:GetService("ReplicatedStorage").HumanoidDied:FireServer(unpack(args))
end

function Royale()
    loadstring(
                game:HttpGet("https://raw.githubusercontent.com/cheesynob39/Roblox-Scripts/main/Slap_Royale.lua"
                    ))()
end

function HitBox()
	loadstring(game:HttpGet(("https://gist.githubusercontent.com/stellar-4242/430ef3087d8d87eb306ca03e728ffbb8/raw/798429dd908b1f4471a1fa569ff62c5e5a93ec61/SLAP.LUA")))()
end

function Reach()
	loadstring(game:HttpGet("https://pastebin.com/raw/jxS60JEF", true))()
end

function AutoUno()
	while wait(6) do
		game:GetService("ReplicatedStorage").ReverseAbility:FireServer()
	end
end

function FarmSlapples()
	loadstring(game:HttpGet("https://pastebin.com/raw/9cxfguU6", true))()
end

function Slapples()
	loadstring(game:HttpGet("https://pastebin.com/raw/rYYpHvDG", true))()
end

--Tabs
local AbilityTab = Window:MakeTab({
	Name = "Ability",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local CombatTab = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local FarmTab = Window:MakeTab({
	Name = "AutoFarm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



--KeyBind
AbilityTab:AddBind({
	Name = "Golden [GodMode]",
	Default = Enum.KeyCode.C,
	Hold = false,
	Callback = function()
		_G.Golden = true
        Golden()
	end    
})

AbilityTab:AddBind({
	Name = "Diamond [Use on Battle]",
	Default = Enum.KeyCode.F,
	Hold = false,
	Callback = function()
		_G.Therock = true
        Therock()
	end    
})

AbilityTab:AddBind({
	Name = "UnGolden [BOB?!]",
	Default = Enum.KeyCode.V,
	Hold = false,
	Callback = function()
		_G.Nogolden = Values
		NoGolden()
	end    
})

AbilityTab:AddBind({
	Name = "GhostOFF [now u can see me]",
	Default = Enum.KeyCode.T,
	Hold = false,
	Callback = function()
		_G.GhostOFF = Value
		GhostOFF()
	end    
})

AbilityTab:AddBind({
	Name = "GhostON [and now u cant see me]",
	Default = Enum.KeyCode.R,
	Hold = false,
	Callback = function()
		_G.GhostON = Value
		GhostON()
	end    
})

AbilityTab:AddBind({
	Name = "PhaseA [UNDERTALE]",
	Default = Enum.KeyCode.Q,
	Hold = false,
	Callback = function()
		_G.PhaseA = Value
		PhaseA()
	end    
})

